  "dependencies": [
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">=1.4.0 <2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.6.0 <5.0.0"
